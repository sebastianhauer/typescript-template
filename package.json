{
  "name": "typescript-template",
  "version": "0.1.0",
  "description": "A TypeScript Node.js template.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run format:check && npm run lint",
    "fix": "npm run format && npm run lint:fix",
    "fix:staged": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastianhauer/typescript-template.git"
  },
  "keywords": [
    "TypeScript",
    "Node.js",
    "node",
    "nodejs"
  ],
  "author": "Sebastian Hauer",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/sebastianhauer/typescript-template/issues"
  },
  "homepage": "https://github.com/sebastianhauer/typescript-template#readme",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.8.4",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2"
  }
}
